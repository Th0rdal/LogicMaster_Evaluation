FROM ai_base

# Copy your source code into the container
COPY src/qLearning ./src/qLearning

# Install Python dependencies if you have a requirements.txt
COPY qlearning_requirements.txt .
RUN pip install --no-cache-dir -r qlearning_requirements.txt

# Command to run your Python AI program
CMD ["python", "src/qLearning/qLearning.py"]
